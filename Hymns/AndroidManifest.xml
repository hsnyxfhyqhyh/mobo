        android:allowBackup="true"
        android:icon="@drawable/hymn_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.chccc.hymn.HymnMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.chccc.hymn.HymnNumberActivity" />
         
         <activity
            android:name="com.chccc.hymn.HymnNumberMultipleActivity" />
    </application>

    
</manifest>
